#elgible for vote or not print yes or no
def age(age):
    if age>18:
       print('elgible')
    else:
       print('not elgible')
age = int(input('enter age:'))
age(age) 


def age(age):
    if age>18:
       return 'True'
    else:
       return 'False'
age = int(input('enter age:'))
if (age(age)):
    print('elgible')
else:
    print('not elgible')
